{
    "name": "razorbacks/style",
    "description": "University of Arkansas layout and style",
    "keywords": ["Arkansas", "style", "layout", "uark"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Jeff Puckett",
            "email": "jpucket@uark.edu"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "razorbacks\\style\\": "php/src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "razorbacks\\style\\laravel\\StyleServiceProvider"
            ]
        }
    }
}
